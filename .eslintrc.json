{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true
    },
    "extends": ["airbnb", "prettier", "prettier/react"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": ["error"]
      }
}